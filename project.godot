; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Friday Night Funkin' Benjine"
config/description="The Benjine source + tools to mod it with!"
run/main_scene="res://scenes/shared/menus/DescriptionLoader.tscn"
config/features=PackedStringArray("4.3")
run/delta_smoothing=false
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/graphics/menus/loading_screens/benjine_simple_splash.png"
config/icon="res://benjine_icon.png"
config/windows_native_icon="res://benjine_app_icon.ico"
run/bg_color=Color(0, 0, 0, 1)

[autoload]

GodotX="*res://scripts/autoload/GodotX.gd"
UserData="*res://scripts/autoload/UserData.gd"
Loader="*res://scenes/autoload/Loader.tscn"
VolumeChanger="*res://scenes/autoload/VolumeChanger.tscn"
Conductor="*res://scenes/autoload/Conductor.tscn"
TransitionSystem="*res://scenes/autoload/TransitionSystem.tscn"
Debug="*res://scripts/autoload/Debug.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="2d"
window/vsync/vsync_mode=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/godot-openxr/plugin.cfg", "res://addons/imrp/plugin.cfg")

[gdnative]

singletons=["res://addons/godot-openxr/config/godot_openxr.gdnlib"]

